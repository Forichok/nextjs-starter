    "mobx-state-tree": "5.1.0",
    "next": "12.0.4",
    "next-images": "1.8.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "4.3.1",
    "react-modal": "3.14.4",
    "react-pdf": "5.6.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@emotion/babel-preset-css-prop": "11.2.0",
